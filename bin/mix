#!/bin/bash
rdlkf() { [ -L "$1" ] && (local lk="$(readlink "$1")"; local d="$(dirname "$1")"; cd "$d"; local l="$(rdlkf "$lk")"; ([[ "$l" = /* ]] && echo "$l" || echo "$d/$l")) || echo "$1"; }
DIR="$(dirname "$(rdlkf "$0")")"
export NODE_PATH=$NODE_PATH:./lib
/usr/bin/env node --harmony --harmony_proxies -e "var fs = require('fs'); fs.existsSync('newrelic.js') && require('newrelic'); require('livescript'); require('$DIR/../lib/command').init().run();" $@
